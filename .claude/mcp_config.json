{
  "mcpServers": {
    "dart": {
      "command": "npx",
      "args": ["dart-mcp-server"]
    },
    "supabase": {
      "command": "npx",
      "args": ["@supabase/mcp-server-supabase"],
      "env": {
        "SUPABASE_URL": "${SUPABASE_URL}",
        "SUPABASE_ANON_KEY": "${SUPABASE_ANON_KEY}"
      }
    },
    "filesystem": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-filesystem", "D:\\Snaplook"]
    },
    "notion": {
      "command": "npx",
      "args": ["@notionhq/notion-mcp-server"],
      "env": {
        "NOTION_API_KEY": "${NOTION_API_KEY}"
      }
    },
    "sentry": {
      "command": "npx",
      "args": ["@sentry/mcp-server"],
      "env": {
        "SENTRY_ORG": "${SENTRY_ORG}",
        "SENTRY_PROJECT": "${SENTRY_PROJECT}",
        "SENTRY_AUTH_TOKEN": "${SENTRY_AUTH_TOKEN}"
      }
    },
    "render": {
      "command": "npx",
      "args": ["-y", "@betterhunt/render-mcp-server"],
      "env": {
        "RENDER_API_TOKEN": "${RENDER_API_TOKEN}"
      }
    },
    "codemagic": {
      "command": "uv",
      "args": [
        "run",
        "--with", "mcp[cli]",
        "--with", "requests",
        "mcp", "run",
        "D:/codemagic-mcp/codemagic_mcp/server.py"
      ],
      "env": {
        "PYTHONPATH": "D:/codemagic-mcp",
        "CODEMAGIC_API_KEY": "${CODEMAGIC_API_KEY}"
      }
    }
  }
}